{
  "name": "server",
  "version": "1.0.0",
  "description": "a node and express based backend application",
  "main": "index.js",
  "scripts": {
    "start": "DEBUG=socket.io* nodemon index.js 'src/**/*.js'",
    "test": "nyc mocha --timeout 60000 --recursive src",
    "lint": "jshint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sh4bbY/node.git"
  },
  "author": "Sascha Bialon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sh4bbY/node/issues"
  },
  "homepage": "https://github.com/Sh4bbY/node#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "jshint": "2.9.4",
    "mocha": "3.2.0",
    "mockgoose": "7.3.1",
    "nodemon": "1.11.0",
    "nyc": "10.2.0",
    "sinon": "2.1.0"
  },
  "dependencies": {
    "bcrypt": "1.0.2",
    "body-parser": "1.17.1",
    "compression": "1.6.2",
    "express": "4.15.2",
    "express-bearer-token": "2.1.0",
    "express-jwt": "5.1.0",
    "joi": "10.4.1",
    "jsonwebtoken": "7.3.0",
    "log4js": "1.1.1",
    "marked": "0.3.6",
    "mongoose": "4.9.4",
    "socket.io": "1.7.3"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
